<Window x:Class="ScriptrunokV2.KeyLoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:ScriptrunokV2"
        mc:Ignorable="d"
        Title="KeyLoginWindow" Height="150" Width="300">
    <Grid>
        <StackPanel>
            <TextBlock HorizontalAlignment="Center" FontSize="14" Margin="5">Введите ключ</TextBlock>
            <TextBox Name="KeyTextBox" Margin="5"></TextBox>
            <Button Name="SubmitButton" Width="60" Margin="5" Click="SubmitButton_Click">Ввести</Button>
        </StackPanel>
    </Grid>
</Window>
